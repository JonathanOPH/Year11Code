#Converter Program

# -------------------------
# Subprograms
# -------------------------

def den_binary(number):
  
  binary = ""
  while number > 0:
    remainder = number % 2
    binary = str(remainder) + binary
    number = number // 2
  return binary 

# -------------------------
# Main program
# -------------------------

number = int(input("Enter a denary number"))
print(den_binary(number))
